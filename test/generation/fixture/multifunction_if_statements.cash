contract TransferWithTimeout(int x, int y) {
    function transfer(int a, int b) {
        int d = a + b;
        d = d - a;
        if (d == x) {
            int c = d + b;
            d = a + c;
            require(c > d);
        } else {
            d = a;
        }
        d = d + a;
        require(d == y);
    }

    function timeout(int b) {
        int d = b;
        d = d + 2;
        if (d == x) {
            int c = d + b;
            d = c + d;
            require(c > d);
        }
        d = b;
        require(d == y);
    }
}
