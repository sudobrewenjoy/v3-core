{
  "name": "cashc",
  "description": "Compile Cash Contracts to Bitcoin Cash Script or artifacts",
  "version": "0.3.4",
  "author": "Rosco Kalis <roscokalis@gmail.com>",
  "bin": {
    "cashc": "dist/cashc-cli.js"
  },
  "bugs": {
    "url": "https://github.com/Bitcoin-com/cashscript/issues"
  },
  "contributors": [
    "Gabriel Cardona <gabriel@bitcoin.com>"
  ],
  "dependencies": {
    "@types/semver": "^6.0.2",
    "@types/yargs": "^13.0.0",
    "antlr4ts": "^0.5.0-alpha.3",
    "bitbox-sdk": "^8.8.0",
    "semver": "^6.3.0",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.3.0",
    "typescript": "3.7.5"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "homepage": "https://github.com/Bitcoin-com/cashscript/tree/master/packages/cashc",
  "keywords": [
    "bitcoin",
    "bitcoin cash",
    "cash contracts",
    "cashscript",
    "compiler",
    "programming language",
    "sdk",
    "smart contracts"
  ],
  "license": "MIT",
  "main": "dist/index",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bitcoin-com/cashscript.git"
  },
  "scripts": {
    "antlr": "antlr4ts -visitor -listener src/grammar/CashScript.g4",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc -p tsconfig.build.json",
    "lint": "eslint . --ext .ts --ignore-path ../../.eslintignore",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "test": "jest --config=../../jest.config.js packages/cashc"
  },
  "type": "dist/index"
}
